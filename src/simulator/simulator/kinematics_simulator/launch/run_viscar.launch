<launch>
  <arg name="map_size_x" value="150.0"/>
  <arg name="map_size_y" value="150.0"/>
  <arg name="map_size_z" value="5.0"/>

  <arg name="origin_x" value="-75.0"/>
  <arg name="origin_y" value="-75.0"/>
  <arg name="origin_z" value="0.1"/>

  <param name="map/x_size" value="$(arg map_size_x)"/>
  <param name="map/y_size" value="$(arg map_size_y)"/>
  <param name="map/z_size" value="$(arg map_size_z)"/>
  <param name="map/resolution" value="0.1"/>

  <param name="map/add_floor" value="false"/>
  <param name="map/add_ceiling" value="false"/>

  <param name="sensing/rate" value="1.0"/>

  <node pkg ="map_generator" name ="urban_generator" type ="urban_generator.py" output = "screen" respawn="true" >
      
  </node>

  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find kinematics_simulator)/config/viscar.rviz" required="true" />
</launch>